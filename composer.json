{
  "name": "fortifi/publicapi",
  "description": "Fortifi Business Platform",
  "homepage": "http://fortifi.co",
  "require": {
    "fortifi/microservice": "1.3.4",
    "fortifi/api": "~1.0",
    "packaged/rwd": "~0.0",
    "fortifi/fortifi-api": "7.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Fortifi\\Services\\PublicApi\\": "src/",
      "Fortifi\\Tests\\Services\\PublicApi\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:fortifi/microservice.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:fortifi/datalayer.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:fortifi/api.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:fortifi/fortifi-api.git"
    }
  ]
}
